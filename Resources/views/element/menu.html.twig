<ul class="navbar-nav mr-auto">
    {% for item in admin_list %}
    <li class="nav-item">
        {% if is_granted('ROLE_USER') and (item.admin is not defined or item.admin == false or (item.admin == true and is_granted('ROLE_ADMIN'))) %}
        <a {% if item.target is defined %}target="{{ item.target }}"{% endif %} class="nav-link" href="{{ path(item.url) }}">{{ item.name }}</a>
        {% endif %}
    </li>
    {% endfor %}
</ul>

